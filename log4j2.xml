<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
    
    <Appenders>
    
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout>
                <Pattern>%d{HH:mm} %msg%n</Pattern>
            </PatternLayout>
        </Console>
        
    </Appenders>
    
    
    <Loggers>
    
        <Root level="info">
            <AppenderRef ref="console"/>
        </Root>
                
    </Loggers>
    
</Configuration>